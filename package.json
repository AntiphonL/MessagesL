    "electron-builder": "22.7.0",
    "electron-mocha": "^8.2.2",
    "electron-notarize": "^0.3.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "mocha": "^7.2.0",
    "png2icons": "^2.0.1",
    "prettier": "^2.0.5",
    "source-map-support": "^0.5.19",
    "spectron": "^11.0.0",
    "style-loader": "^1.2.1",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.2",
